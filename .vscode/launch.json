// .vscode/launch.json
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: glados wyoming",
      "type": "debugpy",
      "request": "launch",
      "python": "${command:python.interpreterPath}",
      "program": "${workspaceFolder}/__main__.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "--uri", "tcp://0.0.0.0:10201",
        "--streaming",
        "--debug"
      ]
    }
  ]
}
